         <MountPoints>
            <MountPoint Side="Front" StartX="0.00" StartY="1.00" EndX="3.00" EndY="2.00" Default="true"/>
            <MountPoint Side="Left" StartX="0.00" StartY="1.00" EndX="1.00" EndY="2.00"/>
            <MountPoint Side="Right" StartX="5.00" StartY="1.00" EndX="6.00" EndY="2.00"/>
         </MountPoints>
         <BuildProgressModels>
      		<Model BuildPercentUpperBound="0.50" File="Models\NanobotDrillSystem\SELtdSmallNanobotDrillSystem_Constr1.mwm" />
		      <Model BuildPercentUpperBound="1.00" File="Models\NanobotDrillSystem\SELtdSmallNanobotDrillSystem_Constr2.mwm" />
         </BuildProgressModels>
         <BuildTimeSeconds>30</BuildTimeSeconds>
         <BlockPairName>SELtdNanobotDrillSystem</BlockPairName>
         <MirroringX>None</MirroringX>
         <MirroringY>None</MirroringY>
         <MirroringZ>None</MirroringZ>
         <EdgeType>Light</EdgeType>

         <ResourceSinkGroup>Defense</ResourceSinkGroup>
         <!--Original range set to zero -->
         <SensorRadius>0</SensorRadius>
         <SensorOffset>0</SensorOffset>
         <CutOutRadius>0</CutOutRadius>
         <CutOutOffset>0</CutOutOffset>

         <PrimarySound>BlockModuleEfficiency</PrimarySound>
         <DamageEffectId>212</DamageEffectId>
         <DamagedSound>ParticleHeavyMech</DamagedSound>
         <PCU>200</PCU>
      </Definition>
